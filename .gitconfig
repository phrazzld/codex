[user]
    email = phraznikov@gmail.com
    name = Phaedrus
[push]
    default = simple
[core]
    excludesfile = ~/.gitignore
    editor = vim
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[web]
    browser = google-chrome
[github]
    user = phrazzld
[alias]
    s = status
    a = add -p
    aa = add -A .
    c = commit -m
    h = push
    l = pull
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
